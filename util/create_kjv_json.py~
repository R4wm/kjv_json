import json

def readBible():
    bible_path = "../data/bible.txt"
    bible_file = open(bible_path, 'r')
    count = 0

    while True:
        count += 1
        line = bible_file.readline()
        if not line:
            break
        print("Line{}: {}".format(count, line.strip()))
    bible_file.close()

if __name__ == '__main__':
    readBible()
